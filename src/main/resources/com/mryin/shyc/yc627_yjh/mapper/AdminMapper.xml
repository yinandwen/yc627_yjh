<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mryin.shyc.yc627_yjh.mapper.AdminMapper">
    <!--根据id查询,返回一个Book-->
    <select id="queryAdminByUsername" resultType="com.mryin.shyc.yc627_yjh.pojo.Admin">
      select * from Admin
      where username = #{username} and password = #{password}
   </select>
</mapper>
